{
  "name": "@onesy/icons-material-sharp-react",
  "version": "1.0.3",
  "description": "Icons material sharp for React",
  "repository": "https://github.com/onesy-me/onesy.git",
  "author": "Lazar EriÄ‡ <lazareric1@proton.me>",
  "license": "MIT",
  "private": false,
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "icons",
    "material",
    "material-design",
    "react",
    "frontend",
    "front-end",
    "backend",
    "back-end",
    "web",
    "browser",
    "javascript",
    "js",
    "typescript",
    "node",
    "nodejs",
    "library",
    "onesy",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env ENV=react VERSION=icons-sharp node ../../utils/js/build.js",
    "vi": "yarn version --patch --no-git-tag-version",
    "release": "yarn test && yarn build && yarn vi && yarn vi --cwd ./build && cd ./build && npm publish --tag latest",
    "release-pure": "yarn test && yarn build && cd ./build && npm publish",
    "lint": "tslint -c ../../tslint.json -p tsconfig.json",
    "type": "tsc --noEmit -p tsconfig.json",
    "cov": "cross-env NODE_ENV=test onesy-test",
    "test:pre": "pm2 update && pm2 delete all && pm2 start ../../utils/js/test/app.js && wait-on http://localhost:4000",
    "test:post": "pm2 delete all",
    "test": "yarn lint && yarn type && yarn test:pre && yarn cov && yarn test:post",
    "make": "node utils/js/make",
    "docs": "node utils/js/docs",
    "clear": "node utils/js/clear"
  },
  "peerDependencies": {
    "@onesy/ui-react": "^1.0.74",
    "react": "^18.1.0"
  },
  "devDependencies": {
    "@onesy/ui-react": "^1.0.74"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "build"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22"
  }
}
